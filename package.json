  "license": "ISC",

  "homepage":            "https://github.com/StreetStrider/aux.js",
  "repository": { "url": "https://github.com/StreetStrider/aux.js.git", "type": "git" },
  "bugs":       { "url": "https://github.com/StreetStrider/aux.js/issues" },

  "sideEffects": false,

  "engines":
  {
    "node": ">= 6"
  },

  "scripts":
  {
     "test": "npm run st && npm run unit && npm run cover",

       "st": "eslint .",

     "unit": "npm run dev && npm run mocha",

    "mocha": "cd release/dev && npm run mocha/1",
    "mocha/1": "mocha -r console-ultimate -r test/global test/*.test.js",

    "cover": "cd release/dev && nyc -r text -r lcov --all npm run mocha/1",

      "dev": "gulp --once",
    "release": "gulp --final --to npm"
  },

  "dependencies":
  {
    "@arrows/composition":
      "1"
  },

  "devDependencies":
  {
    "console-ultimate":
      "3",

    "gulp":
      "4",

    "rollup":
      "1",

    "metalpipe":
      "1",


    "eslint":
      "6",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.6.1",

    "eslint-plugin-node":
      "11",


    "mocha":
      "7",

    "chai":
      "4",

    "nyc":
      "15"
  }
}